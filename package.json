  },
  "homepage": "https://github.com/MKIDeveloper/EMCS_New#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.5",
    "postcss": "^8.4.16",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-nested-ancestors": "^3.0.0",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "daisyui": "^2.24.0",
    "flowbite": "^1.5.2",
    "js-plugin-circliful": "^2.0.15",
    "perfect-scrollbar": "^1.5.5"
  }
}
